{
    "manifest_version": 2,
    "name": "SUST_New_Tab",
    "version": "1.0",
    "browser_action": {
        "default_icon": "sust.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "system.storage"
    ],
    "chrome_url_overrides": {
        "newtab": "customNewTab.html"
    }
}